{
  "name": "vue-antd-jeecg",
  "version": "2.1.1",
  "private": true,
  "scripts": {
    "dev": "set NODE_OPTIONS=--openssl-legacy-provider & vue-cli-service serve",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider & cross-env NODE_ENV=production vue-cli-service build",
    "server": "set NODE_OPTIONS=--openssl-legacy-provider & cd server && npm run start",
    "dll": "webpack -p --progress --config ./dev/webpack.dll.conf.js",
    "lint": "vue-cli-service lint",
    "dev-suncare": "cross-env VUE_APP_SYSCODE=suncare vue-cli-service serve",
    "dev-search": "cross-env VUE_APP_SYSCODE=search vue-cli-service serve",
    "dev-insight": "cross-env VUE_APP_SYSCODE=insight vue-cli-service serve",
    "build-suncare": "cross-env NODE_ENV=production VUE_APP_SYSCODE=suncare vue-cli-service build",
    "build-search": "cross-env NODE_ENV=production VUE_APP_SYSCODE=search vue-cli-service build",
    "build-insight": "cross-env NODE_ENV=production VUE_APP_SYSCODE=insight vue-cli-service build"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "2.1.0",
    "@vue/babel-preset-app": "4.4.6",
    "ant-design-vue": "1.7.5",
    "apexcharts": "3.19.3",
    "axios": "0.18.1",
    "chokidar": "^3.0.0",
    "dayjs": "1.8.29",
    "echarts": "4.8.0",
    "file-loader": "4.3.0",
    "gojs": "2.1.42",
    "js-cookie": "2.2.1",
    "jsencrypt": "^3.2.1",
    "less-loader": "^4.1.0",
    "lodash": "4.17.19",
    "md5": "2.2.1",
    "pdfjs-dist": "2.5.207",
    "poplar-annotation": "2.0.3",
    "register-service-worker": "1.7.1",
    "tinymce": "5.4.1",
    "v-viewer": "1.6.3",
    "viser-vue": "2.4.8",
    "vue": "2.6.11",
    "vue-apexcharts": "1.6.0",
    "vue-class-component": "6.3.2",
    "vue-cropper": "0.4.9",
    "vue-draggable-resizable": "2.2.0",
    "vue-i18n": "8.18.2",
    "vue-loader": "15.9.3",
    "vue-ls": "3.2.1",
    "vue-pdf": "4.2.0",
    "vue-photo-preview": "1.1.3",
    "vue-property-decorator": "7.3.0",
    "vue-router": "3.1.6",
    "vuedraggable": "2.24.3",
    "vuex": "3.6.2",
    "vuex-class": "0.3.2",
    "vxe-table": "^3.3.10",
    "xe-utils": "^3.3.1"
  },
  "devDependencies": {
    "@babel/polyfill": "7.10.4",
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-plugin-pwa": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "@vue/eslint-config-standard": "4.0.0",
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "1.0.1",
    "compression": "1.7.4",
    "core-js": "3.6.5",
    "cross-env": "^5.2.1",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.2.3",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "6.0.0",
    "less": "3.12.2",
    "vue-image-swipe": "1.0.5",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-mixed-operators": 0,
      "vue/max-attributes-per-line": [
        2,
        {
          "singleline": 5,
          "multiline": {
            "max": 1,
            "allowFirstLine": false
          }
        }
      ],
      "vue/attribute-hyphenation": 0,
      "vue/html-self-closing": 0,
      "vue/component-name-in-template-casing": 0,
      "vue/html-closing-bracket-spacing": 0,
      "vue/singleline-html-element-content-newline": 0,
      "vue/no-unused-components": 0,
      "vue/multiline-html-element-content-newline": 0,
      "vue/html-closing-bracket-newline": 0,
      "vue/no-parsing-error": 0,
      "vue/no-unused-vars": 0,
      "no-console": 0
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
